<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">
	<com.google.android.maps.MapView
		android:id="@+id/mapview1" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:clickable="true"
		android:apiKey="@string/debug_map_key"
		android:layout_alignParentBottom="true" android:layout_alignParentTop="true" />
	<ImageView android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:src="@drawable/forward_bg"
		android:id="@+id/imageView_forwad" android:layout_alignTop="@+id/imageView_back"
		android:layout_alignParentRight="true" android:layout_marginRight="37dp"></ImageView>
	<ImageView android:layout_height="wrap_content"
		android:layout_width="wrap_content" android:src="@drawable/back_bg"
		android:id="@+id/imageView_back" android:layout_alignParentBottom="true"
		android:layout_alignParentLeft="true" android:layout_marginLeft="43dp"
		android:layout_marginBottom="32dp"></ImageView>
</RelativeLayout>
